doctype html
html(lang="en")
	head
		title= "Footer 9"
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		link(href="https://fonts.googleapis.com/css?family=Cabin:600", rel="stylesheet")
		link(rel="stylesheet", type= "text/css", href="../../../../css/templates/ui-kit/footers/footer9/style.css")
	body
		section(class="c-footer")
			div(class="c-footer__logo")
				img(src="images/logo.png", alt="logo", class="c-footer__logo-img")
			nav(class="c-footer__menu-item")
				a(href="#", class="c-footer__menu-subitem") Joseph Wood
				a(href="#", class="c-footer__menu-subitem") Timeline
				a(href="#", class="c-footer__menu-subitem") Messages
				a(href="#", class="c-footer__menu-subitem") Statistic
				a(href="#", class="c-footer__menu-subitem") Settings
				a(href="#", class="c-footer__menu-subitem") Logout
			nav(class="c-footer__menu-item")
				a(href="#", class="c-footer__menu-subitem") Services
				a(href="#", class="c-footer__menu-subitem") Branding
				a(href="#", class="c-footer__menu-subitem") UX Design
				a(href="#", class="c-footer__menu-subitem") Prototype
				a(href="#", class="c-footer__menu-subitem") UI Design
				a(href="#", class="c-footer__menu-subitem") Development
			nav(class="c-footer__menu-item")
				a(href="#", class="c-footer__menu-subitem") About Us
				a(href="#", class="c-footer__menu-subitem") Company
				a(href="#", class="c-footer__menu-subitem") Our Team
				a(href="#", class="c-footer__menu-subitem") Contacts
				a(href="#", class="c-footer__menu-subitem") Terms and Conditions
				a(href="#", class="c-footer__menu-subitem") Privacy Policy
			div(class="c-footer__other")
				div(class="c-footer__subscribe")
					span(class="c-footer__subscribe-title") Subscribing
					form(class="c-footer__subscribe-wrapper")
						div(class="c-footer__subscribe-input-wrapper")
							input(type="text", placeholder="E-mail", value="hello@creativeda.sh", class="c-footer__subscribe-input")
						div(class="c-footer__subscribe-submit-wrapper")
							input(type="submit", value="", class="c-footer__subscribe-submit")
				div(class="c-footer__social")
					span(class="c-footer__social-title") Follow us
					button(class="c-footer__social-item c--facebook") 
						img(src="images/facebook.svg", alt="facebook", class="c-footer__social-item-img c--facebook")
					button(class="c-footer__social-item c--twitter") 
						img(src="images/twitter.svg", alt="twitter", class="c-footer__social-item-img c--twitter")
					button(class="c-footer__social-item c--dribbble") 
						img(src="images/dribbble.svg", alt="dribbble", class="c-footer__social-item-img c--dribbble")
					button(class="c-footer__social-item c--vimeo") 
						img(src="images/vimeo.svg", alt="vimeo", class="c-footer__social-item-img c--vimeo")
					button(class="c-footer__social-item c--google-plus") 
						img(src="images/google-plus.svg", alt="google-plus", class="c-footer__social-item-img c--google-plus")